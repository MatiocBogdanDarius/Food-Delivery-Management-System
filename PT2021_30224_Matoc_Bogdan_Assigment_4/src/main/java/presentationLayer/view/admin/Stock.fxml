            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="presentationLayer.controller.sceneControllers.admin.StockController">
    <children>
        <Pane prefHeight="40.0" prefWidth="850.0" styleClass="menuPane" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="homeButton" mnemonicParsing="false" prefHeight="34.0" prefWidth="72.0" text="Home"/>
                <Button fx:id="StockButton" layoutX="72.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="72.0"
                        styleClass="currentSceneButton" text="Stock"/>
                <Button fx:id="updateMenuButton" layoutX="144.0" mnemonicParsing="false" prefHeight="34.0"
                        prefWidth="98.0" text="Update Menu"/>
                <Button fx:id="createMenuButton" layoutX="242.0" mnemonicParsing="false" prefHeight="34.0"
                        prefWidth="98.0" text="Create Menu"/>
                <Button fx:id="createReportsButton" layoutX="340.0" mnemonicParsing="false" prefHeight="34.0"
                        prefWidth="98.0" text="Create Reports"/>
                <TextField fx:id="searchInputTextField" layoutX="450.0" layoutY="5.0" prefHeight="30.0"
