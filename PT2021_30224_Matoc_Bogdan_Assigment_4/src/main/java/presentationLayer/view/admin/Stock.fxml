                <TextField fx:id="caloriesTextField" layoutX="270.0" layoutY="8.0" prefHeight="25.0" prefWidth="60.0"
                           promptText="Calories"/>
                <TextField fx:id="fatTextField" layoutX="410.0" layoutY="8.0" prefHeight="26.0" prefWidth="60.0"
                           promptText="Fat"/>
                <TextField fx:id="sodiumTextField" layoutX="480.0" layoutY="8.0" prefHeight="25.0" prefWidth="60.0"
                           promptText="Sodium"/>
                <TextField fx:id="priceTextField" layoutX="550.0" layoutY="8.0" prefHeight="25.0" prefWidth="60.0"
                           promptText="Price"/>
            </children>
        </AnchorPane>
        <Label fx:id="errorLabel" layoutX="21.0" layoutY="48.0" AnchorPane.leftAnchor="20.0"
               AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="45.0"/>
    </children>
</AnchorPane>
