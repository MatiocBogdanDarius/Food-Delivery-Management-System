                <Button fx:id="searchButton" layoutX="720.0" layoutY="6.0" mnemonicParsing="false" prefHeight="28.0"
                        prefWidth="30.0" AnchorPane.bottomAnchor="17.0" AnchorPane.leftAnchor="320.0">
                    <styleClass>
                        <String fx:value="searchButton"/>
                        <String fx:value="searchInStockButton"/>
                    </styleClass>
                </Button>
            </children>
        </Pane>
        <Button fx:id="logOutButton" layoutX="85.0" layoutY="87.0" mnemonicParsing="false" prefHeight="34.0"
                prefWidth="65.0" text="Log Out" textAlignment="CENTER" wrapText="true" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0"/>

        <TableView fx:id="table" layoutX="14.0" layoutY="45.0" prefHeight="294.0" prefWidth="577.0"
                   AnchorPane.bottomAnchor="65.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                   AnchorPane.topAnchor="65.0"/>
        <AnchorPane layoutX="10.0" layoutY="335.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="41.0"
                    prefWidth="580.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="10.0"
                    AnchorPane.rightAnchor="10.0">
            <children>
                <Button fx:id="deleteButton" alignment="CENTER" layoutX="758.0" layoutY="3.0" mnemonicParsing="false"
                        prefHeight="34.0" prefWidth="72.0" text="Delete" AnchorPane.rightAnchor="15.0"/>
